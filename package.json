{
  "name": "rapidrepo",
  "version": "1.0.0",
  "description": "Multi-tenant SaaS platform",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install",
    "start": "node server/index.js",
    "_comment": "Database seeding scripts",
    "seed:plans": "node server/scripts/seedPlans.js",
    "seed:plans:force": "node server/scripts/seedPlans.js --force",
    "seed:plans:dry-run": "node server/scripts/seedPlans.js --dry-run"
  },
  "keywords": [
    "saas",
    "multi-tenant",
    "rapidrepo"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^12.2.0",
    "browser-image-compression": "^2.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "jimp": "^0.22.12",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.4",
    "react-native-segmented-control": "^1.1.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "nodemon": "^3.0.1"
  }
}
